        <servlet-name>UpdateProductController</servlet-name>
        <url-pattern>/update-product</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ApprovePurchaseRequestServlet</servlet-name>
        <url-pattern>/approvepurchaserequest</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ListPurchaseOrderServlet</servlet-name>
        <url-pattern>/listpurchaseorder</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>PurchaseOrderFormServlet</servlet-name>
        <url-pattern>/purchaseorderform</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SubmitPurchaseOrderServlet</servlet-name>
        <url-pattern>/submitpurchaseorder</url-pattern>
    </servlet-mapping>
    <!-- ERROR PAGES -->
    <error-page>
        <error-code>404</error-code>
        <location>/error/error404.jsp</location>
    </error-page>
    <error-page>
        <error-code>500</error-code>
        <location>/error/error500.jsp</location>
    </error-page>
    <!-- SESSION CONFIG -->
    <servlet-mapping>
        <servlet-name>ApprovePurchaseQuotedServlet</servlet-name>
        <url-pattern>/approvepurchasequoted</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SubmitRequoteFormServlet</servlet-name>
        <url-pattern>/submitrequoteform</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>RequoteFormServlet</servlet-name>
        <url-pattern>/requoteform</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>15</session-timeout>
    </session-config>
</web-app>
