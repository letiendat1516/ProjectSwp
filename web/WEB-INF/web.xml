    <!-- Category Servlet -->
    <servlet>
        <servlet-name>CategoryServlet</servlet-name>
        <servlet-class>controller.CategoryServlet</servlet-class>
    </servlet>

    <!-- Authentication Servlets -->
    <servlet>
        <servlet-name>LoginServlet</servlet-name>
        <servlet-class>controller.LoginServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LogoutServlet</servlet-name>
        <servlet-class>controller.LogoutServlet</servlet-class>
    </servlet>

    <!-- User Management Servlets -->
    <servlet>
        <servlet-name>AdduserServlet</servlet-name>
        <servlet-class>controller.AdduserServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AdminServlet</servlet-name>
        <servlet-class>controller.AdminServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>EdituserServlet</servlet-name>
        <servlet-class>controller.EdituserServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>UserFilterServlet</servlet-name>
        <servlet-class>controller.UserFilterServlet</servlet-class>
    </servlet>

    <!-- Request Management Servlets -->
    <servlet>
        <servlet-name>LoadingRequestServlet</servlet-name>
        <servlet-class>controller.LoadingRequestServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ListRequestImportController</servlet-name>
        <servlet-class>controller.ListRequestImportController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ImportConfirmController</servlet-name>
        <servlet-class>controller.ImportConfirmController</servlet-class>
    </servlet>

    <!-- Role Management Servlet -->
    <servlet>
        <servlet-name>RoleAssignmentServlet</servlet-name>
        <servlet-class>controller.RoleAssignmentServlet</servlet-class>
    </servlet>

    <!-- Profile Management Servlets -->
    <servlet>
        <servlet-name>ProfileServlet</servlet-name>
        <servlet-class>controller.ProfileServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>EditProfileServlet</servlet-name>
        <servlet-class>controller.EditProfileServlet</servlet-class>
    </servlet>

    <!-- Material Unit Management Servlets -->
    <servlet>
        <servlet-name>MaterialUnitServlet</servlet-name>
        <servlet-class>controller.MaterialUnitServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CreateMaterialUnitServlet</servlet-name>
        <servlet-class>controller.CreateMaterialUnitServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>EditMaterialUnitServlet</servlet-name>
        <servlet-class>controller.EditMaterialUnitServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DeleteMaterialUnitServlet</servlet-name>
        <servlet-class>controller.DeleteMaterialUnitServlet</servlet-class>
    </servlet>

    <!-- Export Request -->
    <servlet>
        <servlet-name>ExportRequestController</servlet-name>
        <servlet-class>controller.ExportRequestController</servlet-class>
    </servlet>

    <!-- Servlet Mappings -->

    <!-- Category Mapping -->
    <servlet-mapping>
        <servlet-name>CategoryServlet</servlet-name>
        <url-pattern>/CategoryServlet</url-pattern>
    </servlet-mapping>

    <!-- Authentication Mappings -->
    <servlet-mapping>
        <servlet-name>LoginServlet</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LogoutServlet</servlet-name>
        <url-pattern>/logout</url-pattern>
    </servlet-mapping>

    <!-- User Management Mappings -->
    <servlet-mapping>
        <servlet-name>AdduserServlet</servlet-name>
        <url-pattern>/adduser</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AdminServlet</servlet-name>
        <url-pattern>/admin</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>EdituserServlet</servlet-name>
        <url-pattern>/edituser</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UserFilterServlet</servlet-name>
        <url-pattern>/userfilter</url-pattern>
    </servlet-mapping>

    <!-- Request Management Mappings -->
    <servlet-mapping>
        <servlet-name>LoadingRequestServlet</servlet-name>
        <url-pattern>/loadingrequest</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ListRequestImportController</servlet-name>
        <url-pattern>/import</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ImportConfirmController</servlet-name>
        <url-pattern>/import-confirm</url-pattern>
    </servlet-mapping>

    <!-- Role Management Mapping -->
    <servlet-mapping>
        <servlet-name>RoleAssignmentServlet</servlet-name>
        <url-pattern>/roleAssignment</url-pattern>
    </servlet-mapping>

    <!-- Profile Management Mappings -->
    <servlet-mapping>
        <servlet-name>ProfileServlet</servlet-name>
        <url-pattern>/profile</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>EditProfileServlet</servlet-name>
        <url-pattern>/editprofile</url-pattern>
    </servlet-mapping>

    <!-- Material Unit Management Mappings -->
    <servlet-mapping>
        <servlet-name>MaterialUnitServlet</servlet-name>
        <url-pattern>/material_unit/materialUnit</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CreateMaterialUnitServlet</servlet-name>
        <url-pattern>/material_unit/createMaterialUnit</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>EditMaterialUnitServlet</servlet-name>
        <url-pattern>/material_unit/editMaterialUnit</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DeleteMaterialUnitServlet</servlet-name>
        <url-pattern>/material_unit/deleteMaterialUnit</url-pattern>
    </servlet-mapping>

    <!-- Export Request Mapping -->
    <servlet-mapping>
        <servlet-name>ExportRequestController</servlet-name>
        <url-pattern>/exportRequest</url-pattern>
    </servlet-mapping>

    <!-- Session Configuration -->
    <session-config>
        <session-timeout>15</session-timeout>
        <!-- 15 phút -->
    </session-config>

</web-app>

